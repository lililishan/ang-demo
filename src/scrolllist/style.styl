.filterth
  height 50px
  line-height 30px
  border-bottom 1px solid #eee

.filterth
  .th-date
    padding-top 20px
    line-height 30px
    display inline-block
.filter-content    
  height 35px
  line-height 36px
  border-bottom 1px solid #eee
  background-color #fff
  .rate-col
    color #e53935
  .btn-box
    .btnTxt
      color #1c8acd
      font-size 12px
      margin-right 10px
      display inline-block
.ellipsis 
  white-space: nowrap;
  overflow: hidden;
  -ms-text-overflow: ellipsis;
  text-overflow: ellipsis;