.tableBox
  border 1px solid red
  overflow-y hidden
  &.scroll
    display: flex;
    flex-wrap: nowrap;
    flex-direction: column;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
    -ms-overflow-style: -ms-autohiding-scrollbar;
    .ad-table
      display: flex;
  .flex 
    display: flex;
    flex-wrap: nowrap; 
  .filter-content,.filterth    
    display flex
    flex-wrap: nowrap; 
.ad-table
  // width: 120%;
  border-collapse: collapse;
  border-spacing: 0;
.td-span
  padding-left:20px
  flex: 0 0 auto;
